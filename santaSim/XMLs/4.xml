<santa xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="santa.xsd">

        <replicates>1</replicates>

        <simulation>

            <genome>
                <length>19025</length>
	            <sequences>
                
                </sequences>   
           </genome>


            <population>
                <populationSize>1000</populationSize>
                <inoculum>all</inoculum>
            </population>

            <populationType>dynamicPopulation</populationType>

            <growthModel>
                <growthRate>1.2</growthRate>
                <carryingPopulation>30000</carryingPopulation>
            </growthModel>             

            <fitnessFunction>   
                <ageDependentFitness>                
                    <sites>1500-1503, 1525-1528, 14000-14003, 14200-14206</sites>
                    <declineRate>0.001</declineRate>                    
                </ageDependentFitness>

                <purifyingFitness>                                    
                    <sites>600-606, 803-809, 3000-3009, 7300-7303, 8498-8501, 8803-8806, 17050-17056, 18000-18006</sites>
                    <rank>
                        <order>observed</order>
                        <breakTies>random</breakTies>
                    </rank>
                    <fitness>
                        <lowFitness>0.7</lowFitness>
                        <minimumFitness>0.1</minimumFitness>
                    </fitness>                   
                </purifyingFitness> 
            </fitnessFunction>

	        <mutator>
	            <nucleotideMutator>
	                <mutationRate>$mutationRate</mutationRate>                                       
                    <!-- 1.0E-4 3E-2 2E-2 -->
	                <transitionBias>2.2</transitionBias>
	            </nucleotideMutator>
	        </mutator>

	        <replicator>
	            <recombinantReplicator>
			<dualInfectionProbability>0.1</dualInfectionProbability>
			<recombinationProbability>$recombinationProbability</recombinationProbability>
		      </recombinantReplicator>
	        </replicator>

            <epoch>
                <name>epoch1</name>
                <generationCount>$generationCount</generationCount>
            </epoch>

           
            <samplingSchedule>      
                
                <!-- Sample genotypes at certain times in FASTA Format -->
                <sampler>
                    <atFrequency>$generationCount</atFrequency>
                    <fileName>alignment_0.fa</fileName>
                    <alignment>
                        <sampleSize>$sampleSize</sampleSize>
                        <format>FASTA</format>
                        <label>seq_%g_%s</label>
                    </alignment>
                </sampler>   

            </samplingSchedule>

        </simulation>
</santa>
