<santa xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="santa.xsd">

        <replicates>1</replicates>

        <simulation>

            <genome>
                <length>9181</length>
	            <sequences>
                
                </sequences>   
           </genome>


            <population>
                <populationSize>4000</populationSize>
                <inoculum>all</inoculum>
            </population>            

            <fitnessFunction> 
                <exposureDependentFitness>                    
                    <sites>1000-1006, 6000-6003, 6503-6506, 500-506, 8600-8603, 8700-8709</sites>
                    <penalty>0.02</penalty>
                </exposureDependentFitness>                 
            </fitnessFunction>

            <recombinationHotSpots>
                <recombinationHotSpot>                
                    <feature>genome</feature>
                    <sites>4000,4200</sites>
                    <boostFactor>8</boostFactor>
                </recombinationHotSpot> 

                <recombinationHotSpot>                
                    <feature>genome</feature>
                    <sites>9000,9100</sites>
                    <boostFactor>4</boostFactor>
                </recombinationHotSpot> 

                <recombinationHotSpot>                
                    <feature>genome</feature>
                    <sites>6200,6300</sites>
                    <boostFactor>0.1</boostFactor>
                </recombinationHotSpot>
            </recombinationHotSpots>

	        <mutator>
	            <nucleotideMutator>
	                <mutationRate>$mutationRate</mutationRate>                    
                    <!-- 1.0E-4 3E-2 2E-2 -->
	                <rateBias>
                        0.42 2.49 0.29
                        1.73 0.23 4.73
                        6.99 0.20 0.60
                        1.02 2.56 0.88
                    </rateBias>
	            </nucleotideMutator>
	        </mutator>

	        <replicator>
	            <recombinantReplicator>
			<dualInfectionProbability>0.1</dualInfectionProbability>
			<recombinationProbability>$recombinationProbability</recombinationProbability>
		      </recombinantReplicator>
	        </replicator>

            <epoch>
                <name>epoch1</name>
                <generationCount>$generationCount</generationCount>
            </epoch>

           
            <samplingSchedule>      
                
                <!-- Sample genotypes at certain times in FASTA Format -->
                <sampler>
                    <atFrequency>$generationCount</atFrequency>
                    <fileName>alignment_0.fa</fileName>
                    <alignment>
                        <sampleSize>$sampleSize</sampleSize>
                        <format>FASTA</format>
                        <label>seq_%g_%s</label>
                    </alignment>
                </sampler>   

            </samplingSchedule>

        </simulation>
</santa>
