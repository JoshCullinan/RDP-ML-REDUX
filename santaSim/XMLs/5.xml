<santa xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="santa.xsd">

        <replicates>1</replicates>

        <simulation>

            <genome>
                <length>10727</length>
	            <sequences>
                
                </sequences>   
           </genome>


            <population>
                <populationSize>5000</populationSize>
                <inoculum>all</inoculum>
            </population>                       

            <fitnessFunction>  
                <purifyingFitness>                                    
                    <sites>900-906, 5300-5303</sites>
                    <rank>
                        <order>observed</order>
                        <breakTies>random</breakTies>
                    </rank>
                    <fitness>
                        <lowFitness>0.5</lowFitness>
                        <minimumFitness>0.1</minimumFitness>
                    </fitness>                   
                </purifyingFitness> 
            </fitnessFunction>

	        <mutator>
	            <nucleotideMutator>
                    <mutationRate>$mutationRate</mutationRate>
                    <indelmodel model="NB">0.4 1</indelmodel>
                    <insertprob>3.5E-1</insertprob>
                    <deleteprob>3.5E-1</deleteprob>
                    <!-- 1.0E-4 3E-2 2E-2 -->
                    <rateBias>
                        0.42 2.49 0.29
                        1.73 0.23 4.73
                        6.99 0.20 0.60
                        1.02 2.56 0.88
                    </rateBias>
                </nucleotideMutator>
	        </mutator>

	        <replicator>
	            <recombinantReplicator>
			<dualInfectionProbability>0.1</dualInfectionProbability>
			<recombinationProbability>$recombinationProbability</recombinationProbability>
		      </recombinantReplicator>
	        </replicator>

            <epoch>
                <name>epoch1</name>
                <generationCount>$generationCount</generationCount>
            </epoch>

           
            <samplingSchedule>      
                
                <!-- Sample genotypes at certain times in FASTA Format -->
                <sampler>
                    <atFrequency>$generationCount</atFrequency>
                    <fileName>alignment_0.fa</fileName>
                    <alignment>
                        <sampleSize>$sampleSize</sampleSize>
                        <format>FASTA</format>
                        <label>seq_%g_%s</label>
                    </alignment>
                </sampler>   

            </samplingSchedule>

        </simulation>
</santa>
